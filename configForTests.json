{
    "log4js": {
        "appenders": {
            "out": {
                "type": "stdout"
            },
            "app": {
                "type": "file",
                "filename": "log4js/shoper.log",
                "maxLogSize": 10485760,
                "numBackups": 3
            }
        },
        "categories": {
            "default": {
                "appenders": [
                    "app"
                ],
                "level": "trace"
            }
        }
    },
    "encoding": "utf8",
    "parserOptions": {
        "delimiter": ";",
        "columns": true,
        "skip_empty_lines": true,
        "skip_lines_with_error": true,
        "trim": true,
        "cast": true
    },
    "fileInfo": {
        "path": "tmp",
        "fileName": "test4.csv"
    },
    "errorDelayTime": 5000
}