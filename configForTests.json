{
    "log4js": {
        "appenders": {
            "out": {
                "type": "stdout"
            },
            "app": {
                "type": "file",
                "filename": "log4js/shoper.log",
                "maxLogSize": 10485760,
                "numBackups": 3
            }
        },
        "categories": {
            "default": {
                "appenders": [
                    "app"
                ],
                "level": "trace"
            }
        }
    },
    "encoding": "utf8",
    "parserOptions": {
        "delimiter": ";",
        "columns": true,
        "skip_empty_lines": true,
        "skip_lines_with_error": true,
        "trim": true,
        "cast": true
    },
    "fileInfo": {
        "path": "tmp",
        "fileName": "test4.csv"
    },
    "errorDelayTime": 50,
    "attempsWhenError": 5,
    "shoperConfig": {
        "urls": {
            "token": "",
            "productStocks": "",
            "products": "",
            "productStocksUpdate": ""
        },
        "userToken": "",
        "delayTimeInMilisec": 1000,
        "maxRetryAttempts": 3
    },
    "smtpConfig": {
        "from": "",
        "host": "",
        "port": 587,
        "sercure": false,
        "auth": {
            "user": "",
            "pass": ""
        },
        "ignoreTLS": true,
        "tls": {
            "ciphers": "SSLv3"
          }
    },
    "emailNoticicationList": {
        "alerts": [],
        "adminsNotifications": []
    },
    "backup": {
        "filelocation": "./tmp/backup",
        "status": false
    }
}